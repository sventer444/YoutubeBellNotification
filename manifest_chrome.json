{
    "manifest_version": 2,
    "name": "Youtube Bell Notification",
    "description": "description",
    "content_scripts": [
        {
            "matches": [ "*://*.youtube.com/*" ],
            "js": [ "content-script.js" ],
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "tabs"
    ],
    "web_accessible_resources": [ "player.js" ]
}
